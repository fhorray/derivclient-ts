{
  "name": "derivclient-ts",
  "version": "1.1.1",
  "description": "Standalone Deriv API client",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./indicators": {
      "types": "./dist/indicators/index.d.ts",
      "import": "./dist/indicators/index.js",
      "require": "./dist/indicators/index.js",
      "default": "./dist/indicators/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "indicators": [
        "dist/indicators/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@deriv/deriv-api": "^1.0.12",
    "ws": "^8.16.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/ws": "^8.5.10",
    "@types/node": "^20.11.0"
  }
}
